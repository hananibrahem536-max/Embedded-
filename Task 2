    (3)

#include <stdio.h>

int main() {
    float num1 = 12.25;
    float num2 = 10.12;

    printf("%.2f\n", num1 - (int)num1);
    printf("%.2f\n", num2 - (int)num2);

    return 0;
}

0.25
0.12
